  sh:minCount 1;
  sh:maxCount 1;
  sh:node hri:KindShape;
  dash:viewer dash:DetailsViewer;
  dash:editor dash:BlankNodeEditor .

<http://data.health-ri.nl/core/p2/DataServiceShape#dct:creator> sh:path dcterms:creator;
  sh:name "creator"@en;
  sh:node hri:AgentShape;
  dash:viewer dash:DetailsViewer;
  dash:editor dash:BlankNodeEditor .

