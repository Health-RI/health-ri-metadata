  sh:maxCount 1;
  sh:minCount 1;
  sh:name "Publisher"@en;
  sh:path dct:publisher .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#serves-dataset>
  sh:class dcat:Dataset;
  sh:description "This property refers to a collection of data that this data service can distribute"@en;
  sh:name "serves dataset"@en;
  sh:path dcat:servesDataset .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#Theme> sh:class
    skos:Concept;
  sh:description "A category of the Data Service"@en;
