  sh:name "format"@en;
  sh:path dct:format .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#HVD-Category> sh:class
    skos:Concept;
  sh:name "HVD Category"@en;
  sh:path dcatap:hvdCategory .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#Identifier> sh:class
    rdfs:Literal;
  sh:datatype xsd:string;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "Identifier"@en;
  sh:nodeKind sh:Literal;
  sh:path dct:identifier .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#keyword> sh:class
