    rdfs:Literal;
  sh:datatype xsd:string;
  sh:description "A free-text account of the Data Service"@en;
  sh:minCount 1;
  sh:name "Description"@en;
  sh:nodeKind sh:Literal;
  sh:path dct:description .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#end-point-description>
  sh:class rdfs:Literal;
  sh:datatype xsd:string;
  sh:description "A description of the services available via the end-points, including their operations, parameters etc."@en;
  sh:name "end point description"@en;
  sh:nodeKind sh:Literal;
