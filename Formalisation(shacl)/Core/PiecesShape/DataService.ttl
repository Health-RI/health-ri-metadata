  sh:name "landing Page"@en;
  sh:path dcat:landingPage .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#language> sh:class
    <http://purl.org/dc/terms/LinguisticSystem\u0020(IRI)>;
