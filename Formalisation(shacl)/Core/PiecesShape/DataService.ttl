  sh:maxCount 1;
  sh:minCount 1;
  sh:name "end point URL"@en;
  sh:path dcat:endPointURL .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#format> sh:class
    dct:MediaTypeOrExtent;
