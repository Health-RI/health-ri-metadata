    <http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#other-identifier>,
    <http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#serves-dataset>,
    <http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#Theme>;
  sh:targetClass dcat:DataService .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#Access-Rights>
  dash:editor dash:TextFieldEditor;
  dash:viewer dash:LiteralViewer;
  sh:class dct:RightsStatement;
  sh:description "Information regarding access or restrictions based on privacy, security, or other policies"@en;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "Access Rights"@en;
  sh:path dct:accessRights .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#Applicable-Legislation>
  sh:class "eli:LegalResource";
  sh:description "The legislation that mandates the creation or management of the Data Service"@en;
  sh:name "Applicable Legislation"@en;
  sh:path dcatap:applicableLegislation .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#application-profile>
  sh:class dct:Standard;
  sh:description "An established (technical) standard to which the Data Service conforms"@en;
  sh:name "application profile"@en;
  sh:path dct:conformsTo .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#Contact-Point>
  sh:class vcard:Kind;
  sh:description "Contact information that can be used for sending comments about the Data Service"@en;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "Contact Point"@en;
  sh:path dcat:contactPoint .

<http://data.health-ri.nl/core/p2/DataServiceShapeDataServiceShape#creator> sh:class
    foaf:Agent;
