@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix core: <http://coreRule-healthri.nl#> .
@prefix : <http://coreRule-healthri.nl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .


:DataServiceShape a sh:NodeShape ;
  sh:targetClass dcat:DataService ;
  sh:property [
    sh:path dcat:endpointURL ;
    sh:nodeKind sh:IRI ;
    sh:minCount 1 ;
    sh:name "Endpoint URL" ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ],
  [
    sh:path dct:title ;
	sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    sh:uniqueLang true ;
  ],
  [
    sh:path dct:endpointDescription ;
	sh:nodeKind sh:Literal ;
    sh:uniqueLang true ;
  ],
  [
    sh:path dct:ServesDataset ;
    sh:nodeKind dcat:Dataset ;
  ] .
