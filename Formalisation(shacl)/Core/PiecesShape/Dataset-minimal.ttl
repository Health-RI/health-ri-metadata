<https://data.sparna.fr/shapes-example#> a owl:Ontology;
  dct:description "SHACL mandatory definitions for the Health-RI v2 model for Dataset."@en;
  dct:modified "2025-02-10T15:52:37.650Z"^^xsd:dateTime;
  rdfs:comment "Health-RI v2 Dataset (mandatory)."@en;
  rdfs:label "Dataset"@en;
  owl:versionInfo "0.1" .

hri:DatasetShape a sh:NodeShape;
  rdfs:label "Dataset"@en;
  sh:nodeKind sh:IRI;
  sh:order 1;
  sh:property <http://data.health-ri.nl/core/p2/DatasetShape#dcat:contactPoint>, <http://data.health-ri.nl/core/p2/DatasetShape#dcat:keyword>,
    <http://data.health-ri.nl/core/p2/DatasetShape#dcat:theme>, <http://data.health-ri.nl/core/p2/DatasetShape#dct:accessRights>,
    <http://data.health-ri.nl/core/p2/DatasetShape#dct:creator>, <http://data.health-ri.nl/core/p2/DatasetShape#dct:description>,
    <http://data.health-ri.nl/core/p2/DatasetShape#dct:identifier>, <http://data.health-ri.nl/core/p2/DatasetShape#dct:publisher>,
    <http://data.health-ri.nl/core/p2/DatasetShape#dct:title>;
  sh:targetClass dcat:Dataset;
  shacl-play:background-color "Green" .

<http://data.health-ri.nl/core/p2/DatasetShape#dct:accessRights> dash:editor dash:URIEditor;
  dash:viewer dash:URIViewer;
  sh:description "Information that indicates whether the Dataset is publicly accessible, has access restrictions or is not public"@en;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "access rights"@en;
  sh:nodeKind sh:IRI;
  sh:path dct:accessRights .

<http://data.health-ri.nl/core/p2/DatasetShape#dcat:contactPoint> dash:editor dash:BlankNodeEditor;
  dash:viewer dash:DetailsViewer;
  sh:description "Contact information that can be used for sending comments about the Dataset."@en;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "contact point"@en;
  sh:node hri:KindShapeKindShape;
  sh:path dcat:contactPoint .

<http://data.health-ri.nl/core/p2/DatasetShape#dct:creator> dash:editor dash:BlankNodeEditor;
  dash:viewer dash:DetailsViewer;
  sh:description "An entity responsible for producing the dataset."@en;
  sh:minCount 1;
  sh:name "creator"@en;
  sh:node hri:AgentShapeAgentShape;
  sh:path dct:creator .

<http://data.health-ri.nl/core/p2/DatasetShape#dct:description> dash:editor dash:TextAreaEditor;
  dash:viewer dash:LiteralViewer;
  sh:description "A free-text account of the Dataset."@en;
  sh:minCount 1;
  sh:name "description"@en;
