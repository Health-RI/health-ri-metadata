<http://data.health-ri.nl/core/p2/KindShapeKindShape> a owl:Ontology;
  dct:description "SHACL mandatory definitions for the Health-RI v2 model for vcard:Kind."@en;
  dct:modified "2025-02-10T15:41:02.970Z"^^xsd:dateTime;
  rdfs:comment "Health-RI v2 Kind."@en;
  rdfs:label "Kind"@en;
  owl:versionInfo "0.1" .

<http://data.health-ri.nl/core/p2/KindShape> a sh:NodeShape;
  rdfs:label "Kind"@en;
  sh:nodeKind sh:IRI;
  sh:order 1;
  sh:targetClass vcard:Kind;
  shacl-play:background-color "Green";
  sh:property <http://data.health-ri.nl/core/p2/KindShape#vcard:fn>, <http://data.health-ri.nl/core/p2/KindShape#vcard:hasEmail>,
    <http://data.health-ri.nl/core/p2/KindShape#vcard:hasURL> .

<http://data.health-ri.nl/core/p2/KindShape#vcard:hasURL> dash:editor dash:URIEditor;
  dash:viewer dash:URIViewer;
  sh:description "A webpage that either allows to make contact (i.e. a webform) or the information contains how to get into contact."@en;
  sh:name "contact page"@en;
  sh:nodeKind sh:IRI;
  sh:path vcard:hasURL .

<http://data.health-ri.nl/core/p2/KindShape#vcard:hasEmail> dash:editor dash:URIEditor;
  dash:viewer dash:URIViewer;
  sh:description "A email address via which contact can be made."@en;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "has email"@en;
  sh:nodeKind sh:IRI;
  sh:path vcard:hasEmail .

<http://data.health-ri.nl/core/p2/KindShape#vcard:fn> dash:editor dash:TextAreaEditor;
  dash:viewer dash:LiteralViewer;
  sh:description "The full name of the contact point."@en;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "formatted name"@en;
  sh:nodeKind sh:Literal;
  sh:path vcard:fn .
