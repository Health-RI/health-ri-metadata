@prefix dash: <http://datashapes.org/dash#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix disco: <http://rdf-vocabulary.ddialliance.org/discovery/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix shacl-play: <https://shacl-play.sparna.fr/ontology#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://data.sparna.fr/shapes-example#> a owl:Ontology;
  dct:description "SHACL mandatory definitions for the Health-RI v2 model for Project."@en;
  dct:modified "2025-02-10T15:54:15.890Z"^^xsd:dateTime;
  rdfs:comment "Health-RI v2 Project."@en;
  rdfs:label "Dataset"@en;
  owl:versionInfo "0.1" .

<http://data.health-ri.nl/core/p2/ProjectShape> a sh:NodeShape;
  rdfs:label "Project"@en;
  sh:nodeKind sh:IRI;
  sh:order 1;
  sh:property <http://data.health-ri.nl/core/p2/ProjectShape#dcat:resource>, <http://data.health-ri.nl/core/p2/ProjectShape#dct:description>,
    <http://data.health-ri.nl/core/p2/ProjectShape#dct:hasPart>, <http://data.health-ri.nl/core/p2/ProjectShape#dct:identifier>,
    <http://data.health-ri.nl/core/p2/ProjectShape#dct:title>, <http://data.health-ri.nl/core/p2/ProjectShape#foaf:fundedBy>;
  sh:targetClass foaf:Project;
  shacl-play:background-color "Green" .

<http://data.health-ri.nl/core/p2/ProjectShape#dct:description> dash:editor dash:TextAreaEditor;
  dash:viewer dash:LiteralViewer;
  sh:description "A free text description of the project. Cardinality 1..n to supply the information in multiple languages, if preferred."@en;
  sh:minCount 1;
  sh:name "Description"@en;
  sh:nodeKind sh:Literal;
  sh:path dct:description .

<http://data.health-ri.nl/core/p2/ProjectShape#dct:identifier> dash:editor dash:URIEditor;
  dash:viewer dash:URIViewer;
  sh:description "This property is intended for the GrantID of the project."@en;
  sh:maxCount 1;
  sh:minCount 1;
  sh:name "Identifier"@en;
  sh:nodeKind sh:Literal;
  sh:path dct:identifier .

<http://data.health-ri.nl/core/p2/ProjectShape#dcat:resource> dash:editor dash:URIEditor;
  dash:viewer dash:URIViewer;
  sh:class dcat:Catalog;
  sh:description "This property is intended to connect the project to a catalogue of which the project is part of."@en;
  sh:minCount 1;
  sh:name "Catalogue"@en;
  sh:nodeKind sh:IRI;
  sh:path dcat:resource .

<http://data.health-ri.nl/core/p2/ProjectShape#foaf:fundedBy> dash:editor dash:BlankNodeEditor;
  dash:viewer dash:DetailsViewer;
  sh:description "Use this property to indicate the funding agency/agencies that provide funding for the project. This property uses the class foaf:Agent, which has its own mandatory and recommended properties. In case the project is funded internally, the corresponding institute can be submitted as funding agent."@en;
  sh:minCount 1;
  sh:name "Funder"@en;
  sh:node foaf:Agent;
  sh:nodeKind sh:IRI;
  sh:path foaf:fundedBy .

<http://data.health-ri.nl/core/p2/ProjectShape#dct:title> dash:editor dash:TextFieldEditor;
  dash:viewer dash:LiteralViewer;
  sh:description "Indicate the title of the project here. The title can be supplied in multiple languages if preferred; this is why the cardinality is set to 1..n."@en;
  sh:minCount 1;
  sh:name "Title"@en;
  sh:nodeKind sh:Literal;
  sh:path dct:title .

<http://data.health-ri.nl/core/p2/ProjectShape#dct:hasPart> dash:editor dash:BlankNodeEditor;
  dash:viewer dash:DetailsViewer;
  sh:description "As the study class is used to give more information about data generation or collection, the different data collection endeavors of the project can be stated by instantiating one or several instances of the study class. This property links a project to its study/studies."@en;
  sh:name "Study"@en;
  sh:node disco:Study;
  sh:nodeKind sh:IRI;
  sh:path dct:hasPart .
