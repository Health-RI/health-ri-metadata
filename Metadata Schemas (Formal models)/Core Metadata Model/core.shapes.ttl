  owl:versionInfo "Created with TopBraid Composer" ;
.
core:Agent_Shape
  rdf:type sh:NodeShape ;
  sh:name "Agent"@en ;
  sh:property [
      sh:path dct:type ;
      sh:class skos:Concept ;
      sh:maxCount 1 ;
      sh:severity sh:Violation ;
    ] ;
  sh:property [
      sh:path foaf:name ;
      sh:minCount 1 ;
      sh:nodeKind sh:Literal ;
      sh:severity sh:Violation ;
    ] ;
  sh:targetClass foaf:Agent ;
.
