@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard:<http://www.w3.org/2006/vcard#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dash: <http://datashapes.org/dash#> .

<http://coreRule-healthri.nl> a owl:Ontology ;
    rdfs:comment "This set of rules defines constraints on the Data Catalog Vocabulary (DCAT) for establishing the fundamental requirements for onboarding metadata onto the national catalog" ;
    owl:versionInfo "Created with TopBraid Composer - Version 1.0" ;
    dct:creator "Dena Tahvildari" ;
    owl:imports <http://datashapes.org/dash>,
        <http://purl.org/dc/elements/1.1/>,
        <http://purl.org/dc/terms/>,
        <http://www.w3.org/2004/02/skos/core>,
        <http://www.w3.org/ns/dcat#> .

<http://coreRule-healthri.nl#Agent_Shape> a sh:NodeShape ;
    sh:name "Agent"@en ;
    sh:targetClass foaf:Agent ;
    sh:property [
            sh:path foaf:name ;
            sh:severity sh:Violation ;
            sh:nodeKind sh:Literal ;
            sh:minCount 1
        ], [
            sh:path <http://purl.org/dc/terms/type> ;
            sh:severity sh:Violation ;
            sh:class skos:Concept ;
            sh:maxCount 1
        ] ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 1 .

<http://coreRule-healthri.nl#Catalog_Shape> a sh:NodeShape ;
    sh:property [
        sh:path <http://purl.org/dc/terms/creator>;
        sh:class foaf:Agent;
        sh:maxCount 1;
        sh:severity sh:Violation
    ], [
        sh:path <http://purl.org/dc/terms/description>;
        sh:nodeKind sh:Literal;
        sh:severity sh:Violation;
        sh:minCount 1
    ], [
        sh:path <http://purl.org/dc/terms/hasPart>;
        sh:class <http://www.w3.org/ns/dcat#Catalog>;
        sh:severity sh:Violation
    ], [
        sh:path <http://purl.org/dc/terms/isPartOf>;
        sh:class <http://www.w3.org/ns/dcat#Catalog>;
        sh:maxCount 1;
        sh:severity sh:Violation
    ], [
        sh:path <http://purl.org/dc/terms/license>;
        sh:class <http://purl.org/dc/terms/LicenseDocument>;
        sh:maxCount 1;
        sh:severity sh:Violation
    ], [
        sh:path <http://purl.org/dc/terms/publisher>;
        sh:class foaf:Agent;
        sh:maxCount 1;
        sh:severity sh:Violation;
        sh:minCount 1
    ], [
        sh:path <http://purl.org/dc/terms/rights>;
        sh:class <http://purl.org/dc/terms/RightsStatement>;
        sh:maxCount 1;
        sh:severity sh:Violation
    ], [
        sh:path <http://purl.org/dc/terms/spatial>;
        sh:class <http://purl.org/dc/terms/Location>;
        sh:severity sh:Violation
    ], [
        sh:path <http://purl.org/dc/terms/title>;
        sh:nodeKind sh:Literal;
        sh:severity sh:Violation;
        sh:minCount 1
    ], [
        sh:path foaf:homepage;
        sh:class foaf:Document;
        sh:maxCount 1;
        sh:severity sh:Violation
    ], [
        sh:path <http://www.w3.org/ns/dcat#catalog>;
        sh:class <http://www.w3.org/ns/dcat#Catalog>;
        sh:severity sh:Violation
    ], [
        sh:path <http://www.w3.org/ns/dcat#dataset>;
        sh:class <http://www.w3.org/ns/dcat#Dataset>;
        sh:severity sh:Violation;
        sh:minCount 0
    ], [
        sh:path <http://www.w3.org/ns/dcat#service>;
        sh:class <http://www.w3.org/ns/dcat#DataService>;
        sh:severity sh:Violation
    ], [
        sh:path <http://purl.org/dc/terms/theme>;
        sh:class skos:ConceptScheme;
        sh:severity sh:Violation
    ];
    sh:targetClass <http://www.w3.org/ns/dcat#Catalog>;
    sh:name "Catalog"@en ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 2 .

<http://coreRule-healthri.nl#DataService_Shape> a sh:NodeShape ;
    sh:name "Data Service"@en ;
    sh:targetClass dcat:DataService ;
    sh:property [
            sh:path <http://purl.org/dc/terms/accessRights> ;
            sh:class <http://purl.org/dc/terms/RightsStatement> ;
            sh:maxCount 1 ;
            sh:severity sh:Violation
        ], [
            sh:path <http://purl.org/dc/terms/description> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation
        ], [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:minCount 1
        ], [
            sh:path <http://www.w3.org/ns/dcat#endpointDescription> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:severity sh:Violation
        ], [
            sh:path <http://www.w3.org/ns/dcat#endpointURL> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:severity sh:Violation ;
            sh:minCount 1
        ], [
            sh:path <http://www.w3.org/ns/dcat#servesDataset> ;
            sh:class dcat:Dataset ;
            sh:severity sh:Violation
        ] ;
    sh:targetClass dcat:DataService ;
    sh:name "Data Service"@en ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 3 .

<http://coreRule-healthri.nl#Dataset_Shape> a sh:NodeShape ;
    sh:name "Dataset"@en ;
    sh:targetClass dcat:Dataset ;
    sh:property [
            sh:path <http://purl.org/dc/terms/contactPoint> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:severity sh:Violation ;
            sh:minCount 1 ;
            sh:name "contact point"
        ], [
        sh:path dcat:inSeries ;
            sh:class dcat:DatasetSeries ;
            sh:mimCount 0 ;
            sh:severity sh:Violation
        ], [
            sh:path <http://purl.org/dc/terms/creator> ;
            sh:class foaf:Agent ;
            sh:maxCount 1 ;
            sh:severity sh:Violation
        ], [
            sh:path <http://purl.org/dc/terms/version> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:minCount 1 ;
            sh:name "version"
        ], [
            sh:path <http://purl.org/dc/terms/description> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:minCount 1
        ], [
            sh:path <http://purl.org/dc/terms/identifier> ;
            sh:nodeKind sh:Literal ;
            sh:datatype xsd:string ;
            sh:severity sh:Violation ;
            sh:minCount 1
        ], [
            sh:path <http://purl.org/dc/terms/issued> ;
            sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:severity sh:Violation
        ], [
            sh:path <http://purl.org/dc/terms/modified> ;
            sh:maxCount 1 ;
            sh:datatype xsd:dateTime ;
            sh:severity sh:Violation
        ], [
            sh:path <http://purl.org/dc/terms/publisher> ;
            sh:class foaf:Agent ;
            sh:minCount 1 ;
            sh:severity sh:Violation
        ], [
            sh:path <http://purl.org/dc/terms/temporal> ;
            sh:class <http://purl.org/dc/terms/PeriodOfTime> ;
            sh:severity sh:Violation ;
            sh:minCount 0
        ], [
            sh:path <http://purl.org/dc/terms/theme> ;
            sh:class skos:Concept ;
            sh:severity sh:Violation ;
            sh:minCount 1
        ], [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
            sh:datatype xsd:string ;
            sh:severity sh:Violation ;
            sh:minCount 1
        ], [
            sh:path <http://purl.org/dc/terms/type> ;
            sh:class skos:Concept ;
            sh:severity sh:Violation ;
            sh:minCount 0
        ], [
            sh:path owl:versionInfo ;
            sh:nodeKind sh:Literal ;
            sh:datatype xsd:string ;
            sh:severity sh:Violation ;
            sh:maxCount 1
        ], [
            sh:path <http://www.w3.org/ns/dcat#distribution> ;
            sh:class dcat:Distribution ;
            sh:severity sh:Violation
        ] ;
    sh:targetClass dcat:Dataset ;
    sh:name "Dataset"@en ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 4 .

<http://coreRule-healthri.nl#Distribution_Shape> a sh:NodeShape ;
    sh:name "Distribution"@en ;
    sh:targetClass dcat:Distribution ;
    sh:property [
            sh:path <http://purl.org/dc/terms/description> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation
        ], [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation
        ], [
            sh:path <http://www.w3.org/ns/dcat#accessService> ;
            sh:class dcat:DataService ;
            sh:severity sh:Violation
        ], [
            sh:path <http://www.w3.org/ns/dcat#accessURL> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:severity sh:Violation ;
            sh:minCount 1
        ], [
            sh:path <http://www.w3.org/ns/dcat#downloadURL> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:maxCount 1 ;
            sh:severity sh:Violation
        ], [
            sh:path <http://www.w3.org/ns/dcat#mediaType> ;
            sh:class <http://purl.org/dc/terms/MediaType> ;
            sh:maxCount 1 ;
            sh:severity sh:Violation
        ] ;
    sh:targetClass dcat:Distribution ;
    sh:name "Distribution"@en ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 5 .

<http://coreRule-healthri.nl#Project> a sh:NodeShape ;
    rdfs:label "Project" ;
    sh:property [
            sh:path <http://purl.org/dc/terms/description> ;
            sh:datatype xsd:string ;
            sh:minCount 0 ;
            sh:name "description"
        ], [
            sh:path <http://purl.org/dc/terms/identifier> ;
            sh:datatype xsd:string ;
            sh:minCount 0 ;
            sh:name "identifier"
        ], [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:datatype xsd:string ;
            sh:minCount 0 ;
            sh:name "title"
        ], [
            sh:path foaf:fundedBy ;
            sh:class foaf:Agent ;
            sh:minCount 0 ;
            sh:name "funded by"
        ], [
            sh:path <http://www.w3.org/ns/dcat#dataset> ;
            sh:class dcat:Dataset ;
            sh:minCount 0 ;
            sh:name "dataset"
        ] ;
    sh:targetClass <http://coreRule-healthri.nl#Project> ;
    sh:name "Project" ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 6 .

<http://coreRule-healthri.nl#DataSeries_Shape> a sh:NodeShape ;
    sh:targetClass dcat:DataSeries ;
    sh:property [
            sh:path dcat:title ;
            sh:minCount 1 ;
            sh:severity sh:Violation ;
            sh:message "Title is mandatory for each DataSeries."
    ] ;
    sh:property [
        sh:path dcat:description ;
        sh:minCount 1 ;
        sh:severity sh:Violation ;
        sh:message "Description is mandatory for each DataSeries."
    ] ;
    sh:name "DataSeries" ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 7 .

<http://coreRule-healthri.nl#DatasetSeries_Shape> a sh:NodeShape ;
    sh:targetClass dcat:DatasetSeries ;
    sh:name "DatasetSeries" ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 8 .

<http://coreRule-healthri.nl#CatalogRecord_Shape> a sh:NodeShape ;
    sh:targetClass dcat:CatalogRecord ;
    sh:name "CatalogRecord" ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 9 .

<http://coreRule-healthri.nl#Catalog_Shape> a sh:NodeShape ;
    sh:targetClass dcat:Catalog ;
    sh:name "Catalog" ;
    dash:viewer dash:DetailsViewer ;
    dash:widget dash:DetailsWidget ;
    dash:order 10 .
