#baseURI: http://coreRule-healthri.nl

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix vcard: <http://www.w3.org/2006/vcard#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dash: <http://datashapes.org/dash#> .
@prefix core: <http://coreRule-healthri.nl#> .
@prefix : <http://coreRule-healthri.nl#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

<http://coreRule-healthri.nl> 
   rdf:type owl:Ontology ;
   dc:creator "Dena Tahvildari" ;
   rdfs:comment "This set of rules defines constraints on the Data Catalog Vocabulary (DCAT) for establishing the fundamental requirements for onboarding metadata onto the DUtch National Catalog" ;
   owl:versionInfo "Created with TopBraid Composer - Version 1.0" ;
   owl:imports <http://datashapes.org/dash>,
        <http://purl.org/dc/elements/1.1/>,
        <http://purl.org/dc/terms/>,
        <http://www.w3.org/2004/02/skos/core>,
        <http://www.w3.org/ns/dcat#> 
.
<http://coreRule-healthri.nl#Agent_Shape> a sh:NodeShape ;
    sh:name "Agent"@en ;
    sh:targetClass foaf:Agent ;
    sh:property [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:class rdfs:Literal ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ; 
sh:property [
            sh:path <http://purl.org/dc/terms/identifier> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:class [ owl:unionOf (rdfs:Literal sh:datatype) ] ; 
            sh:minCount 1 ;
            sh:uniqueLang true ;
    ] .
<http://coreRule-healthri.nl#Project_Shape> a sh:NodeShape ;
    sh:name "Project"@en ;
    sh:targetClass foaf:Project ;
  sh:property [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
	    sh:datatype xsd:string ;
            sh:class rdfs:Literal ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ; 
sh:property [ 
            sh:path <http://purl.org/dc/terms/identifier> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:class [ owl:unionOf (rdfs:Literal) ] ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ; 
sh:property [
            sh:path <http://purl.org/dc/terms/description> ;
            sh:nodeKind sh:LiteralOrIRI ;
            sh:severity sh:Violation ;
            sh:class [ owl:unionOf (rdfs:Literal) ] ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ;
sh:property [
           sh:path foaf:fundedBy ;
           sh:nodeKind sh:LiteralOrIRI ;
           sh:severity sh:Violation ;
           sh:class foaf:Agent ;
           sh:minCount 0 ;
           sh:uniqueLang true ;
      ] ;   
sh:property [
           sh:path <http://www.w3.org/ns/dcat#dataset> ;
           sh:class dcat:Dataset ;
           sh:severity sh:Violation ;
           sh:minCount 1 ;
           sh:uniqueLang true ;  
       ] ;  
 sh:property [
           sh:path <http://purl.org/dc/terms/type> ;
           sh:class skos:Concept ;
           sh:severity sh:Violation ;
           sh:minCount 0 ;
           sh:uniqueLang true ;  
	] .
<http://coreRule-healthri.nl#Catalog_Shape> a sh:NodeShape ;
    sh:name "Catalog"@en ;
    sh:targetClass dcat:Catalog ;
    sh:property [
            sh:path <http://purl.org/dc/terms/creator> ;
            sh:class foaf:Agent ;
            sh:maxCount 1 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path <http://purl.org/dc/terms/description> ;
            sh:nodeKind sh:LiteralOrIRI ;
            sh:severity sh:Violation ;
       	    sh:class [ owl:unionOf (rdfs:Literal) ] ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/hasPart> ;
            sh:class dcat:Catalog ;
            sh:severity sh:Violation ;
	    sh:minCount 0 ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path <http://purl.org/dc/terms/license> ;
            sh:class <http://purl.org/dc/terms/LicenseDocument> ;
            sh:maxCount 1 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/publisher> ;
            sh:class foaf:Agent ;
            sh:severity sh:Violation ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path <http://purl.org/dc/terms/spatial> ;
            sh:class <http://purl.org/dc/terms/Location> ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:class rdfs:Literal ;
            sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path foaf:homepage ;
            sh:class foaf:Document ;
            sh:minCount 0 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
            dash:editor dash:URIEditor ;
            dash:viewer dash:LabelViewer ;
        ] ; 
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#catalog> ;
            sh:class dcat:Catalog ;
            sh:severity sh:Violation ;
	    sh:minCount 0 ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#dataset> ;
            sh:class dcat:Dataset ;
            sh:severity sh:Violation ;
            sh:minCount 0 ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#service> ;
            sh:class dcat:DataService ;
            sh:severity sh:Violation ;
            sh:minCount 0 ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path <http://purl.org/dc/terms/theme> ;
            sh:class skos:Concept ;
            sh:severity sh:Violation ;
            sh:minCount 0 ;
            sh:uniqueLang true ;
            dash:viewer dash:LabelViewer ;
        ] .
<http://coreRule-healthri.nl#DataService_Shape> a sh:NodeShape ;
    sh:name "Data Service"@en ;
    sh:targetClass dcat:DataService ;
    sh:property [
            sh:path <http://www.w3.org/ns/dcat#endpointDescription> ;
            sh:nodeKind sh:LiteralOrIRI ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
            sh:class [ owl:unionOf (rdfs:Literal sh:IRI) ] ;
	    sh:minCount 0 ;
            dash:editor dash:TextAreaEditor ;
            dash:viewer dash:LiteralViewer ;
        ]; 
 sh:property [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
	    sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ]; 
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#endpointURL> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:severity sh:Violation ;
            sh:class rdfs:Literal ;
            sh:minCount 0 ;
            sh:uniqueLang true ;
        ]; 
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#servesDataset> ;
            sh:class dcat:Dataset ;
	    sh:minCount 0 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] .
<http://coreRule-healthri.nl#Dataset_Shape> a sh:NodeShape ;
    sh:name "Dataset"@en ;
    sh:targetClass dcat:Dataset ;
    sh:property [
            sh:path <http://purl.org/dc/terms/contactPoint> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:class vcard:Kind ;
            sh:severity sh:Violation ;
            sh:minCount 1 ;
            sh:name "contact point" ;
            sh:uniqueLang true ; 
            dash:editor dash:URIEditor ;
            dash:viewer dash:LabelViewer ;
        ] ; 
 sh:property [
            sh:path dcat:inSeries ;
            sh:class dcat:DatasetSeries ;
            sh:minCount 0 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ]; 
 sh:property [
            sh:path <http://purl.org/dc/terms/creator> ;
            sh:class foaf:Agent ;
            sh:minCount 1 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path dcat:hasVersion ;
	    sh:nodeKind sh:LiteralOrIRI ;
            sh:severity sh:Violation ;
	    sh:class [ owl:unionOf (rdfs:Literal sh:IRI) ] ;
            sh:minCount 0 ;
            sh:name "Has Version" ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
             sh:path <http://purl.org/dc/terms/description> ;
             sh:nodeKind sh:LiteralOrIRI ;
             sh:severity sh:Violation ;
             sh:class [ owl:unionOf (rdfs:Literal sh:IRI) ] ;
             sh:minCount 1 ;
             sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/identifier> ;
            sh:nodeKind sh:Literal ;
            sh:severity sh:Violation ;
            sh:class [ owl:unionOf (rdfs:Literal) ] ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/issued> ;
            sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true;
            dash:editor dash:DatePickerEditor ;
            dash:viewer dash:LiteralViewer 
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/modified> ;
            sh:maxCount 1 ;
            sh:datatype xsd:dateTime ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
            dash:editor dash:DatePickerEditor ;
            dash:viewer dash:LiteralViewer  
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/publisher> ;
            sh:class foaf:Agent ;
            sh:minCount 1 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/temporal> ;
            sh:class <http://purl.org/dc/terms/PeriodOfTime> ;
            sh:severity sh:Violation ;
            sh:minCount 0 ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/theme> ;
            sh:class skos:ConceptScheme ;
            sh:severity sh:Violation ;
            sh:minCount 1 ;
            sh:uniqueLang true ; 
            dash:editor dash:URIEditor ;
            dash:viewer dash:LabelViewer 
        ] ;
 sh:property [
            sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
            sh:datatype xsd:string ;
            sh:severity sh:Violation ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path <http://purl.org/dc/terms/type> ;
            sh:class skos:Concept ;
            sh:severity sh:Violation ;
            sh:minCount 0 ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#distribution> ;
            sh:class dcat:Distribution ;
            sh:minCount 0 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] .
<http://coreRule-healthri.nl#Distribution_Shape> a sh:NodeShape ;
    sh:name "Distribution"@en ;
    sh:targetClass dcat:Distribution ;
    sh:property [
            sh:path <http://purl.org/dc/terms/description> ;
            sh:nodeKind sh:LiteralOrIRI ;
            sh:severity sh:Violation ;
            sh:class [ owl:unionOf (rdfs:Literal sh:IRI) ] ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
    ] ;
 sh:property [
	    sh:path <http://purl.org/dc/terms/title> ;
            sh:nodeKind sh:Literal ;
	    sh:datatype xsd:string ;
            sh:severity sh:Violation ;
	    sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#accessService> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
	    sh:class dcat:DataService ;
	    sh:minCount 0 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#accessURL> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:severity sh:Violation ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#downloadURL> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:maxCount 1 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] ;
 sh:property [
            sh:path <http://www.w3.org/ns/dcat#mediaType> ;
            sh:nodeKind sh:BlankNodeOrIRI ;
	    sh:class <http://purl.org/dc/terms/MediaType> ;
            sh:maxCount 1 ;
            sh:severity sh:Violation ;
            sh:uniqueLang true ;
        ] .
<http://coreRule-healthri.nl#DataSeries_Shape> a sh:NodeShape ;
    sh:name "Data Series"@en ;
    sh:targetClass dcat:DataSeries ;
    sh:property [
            sh:path dcat:title ;
            sh:severity sh:Violation ;
            sh:class rdfs:Literal;
	    sh:datatype xsd:string ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] ; 
 sh:property [
            sh:path dcat:description ;
            sh:severity sh:Violation ;
            sh:class rdfs:Literal ;
            sh:minCount 1 ;
            sh:uniqueLang true ;
        ] .
 <http://coreRule-healthri.nl#Resource_Shape> a sh:NodeShape ;
  sh:name "Resource"@en ;
  sh:targetClass dcat:Resource ;
  sh:property [
    sh:path dct:title ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount  1 ;
    dash:editor dash:TextFieldEditor ;
  ] ; 
 sh:property [
    sh:path dct:description ;
    sh:nodeKind sh:Literal ;
    sh:class rdfs:Literal ;
    sh:maxCount 1 ;
    dash:editor dash:TextAreaEditor ;
  ] ;
 sh:property [
    sh:path dct:publisher ;
    sh:node foaf:Agent ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    dash:editor dash:BlankNodeEditor ;
  ] ;
 sh:property [
    sh:path dcat:hasVersion ;
    sh:name "Has Version" ;
    sh:nodeKind sh:Literal ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    dash:editor dash:TextFieldEditor ;
    dash:viewer dash:LiteralViewer ;
  ] ;
 sh:property [
    sh:path dct:language ;
    sh:nodeKind sh:IRI ;
    sh:maxCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ] ; 
sh:property [
    sh:path dct:license ;
    sh:nodeKind sh:IRI ;
    sh:maxCount 1 ;
    dash:editor dash:URIEditor ;
    dash:viewer dash:LabelViewer ;
  ] . 
